{
	"name": "serve",
	"engines": {
		"node": "^10"
	},
	"dependencies": {
		"@sap/cds": "^4.1.5",
		"@sap/cloud-sdk-core": "^1.17.2",
		"@sap/hana-client": "^2.5.104",
		"@sap/xsenv": "^3.0.0",
		"@sap/xssec": "^2.2.5",
		"express": "^4.17.1",
		"passport": "^0.4.1",
		"uuid": "^8.3.0"
	},
	"devDependencies": {},
	"scripts": {
		"postinstall": "npm dedupe && node .build.js",
		"start:cds:serve": "node ./node_modules/@sap/cds/bin/cds.js serve gen/csn.json",
		"start": "node --inspect express.js"
	},
	"cds": {
		"requires": {
			"db": {
				"kind": "hana",
				"model": "gen/csn.json"
			},
			"EPM_REF_APPS_PROD_MAN_SRV": {
				"kind": "odata",
				"model": "external/EPM_REF_APPS_PROD_MAN_SRV",
				"credentials": {
					"destination": "ES5_CAP",
					"requestTimeout": 30000
				},
				"pool": {
					"min": 1,
					"max": 10
				}
			}
		},
		"auth": {
			"passport": {
				"strategy": "JWT"
			}
		}
	}
}
